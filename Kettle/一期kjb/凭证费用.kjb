      <name>dm_部门科目费用</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>delete from dm_部门科目费用;
insert into dm_部门科目费用

with k1 as(
select 
a.公司主体,
left(a.日期,10) 日期,
a.年,
a.月,
a.科目编码,
a.科目名称,
a.借方金额,
a.贷方金额,
b.明细科目 会计科目,
b.季节性产品,
case when b.业务主体='东莞' and b.费用归属组 in ('财务部','财务管理部') then '财务管理部' 
     when b.业务主体='深圳' and b.费用归属组 in ('人事行政部') then '人力行政部'
else b.费用归属组 end 渠道大类
from ods_金蝶凭证数据 a
left join ods_dm_费用科目表 b on a.id=b.id)

select d.公司主体,d.日期,d.年,d.月,d.科目编码,d.科目名称,
case when d.会计科目='销售费用_寄样运费' then e.样品运费 else e.运费 end 借方金额, 
d.贷方金额, d.会计科目,d.季节性产品, e.部门 渠道大类
from 
(
select distinct 公司主体,日期,年,月,'6602' 科目编码,'销售费用' 科目名称,0 借方金额,null 贷方金额,'销售费用_运费' 会计科目,null 季节性产品,null 渠道大类
from k1 
where 公司主体='深圳市榴芒一刻食品有限公司'

union all 
select distinct 公司主体,日期,年,月,'6602' 科目编码,'销售费用' 科目名称,0 借方金额,null 贷方金额,'销售费用_寄样运费' 会计科目,null 季节性产品,null 渠道大类
from k1 
where 公司主体='深圳市榴芒一刻食品有限公司' 

) d

left join (select 查询日期,日期,
case when a.客户=b.金蝶名称 then b.吉客云名称 else a.客户 end 客户,
case when a.客户=b.金蝶名称 then b.负责部门 else a.部门 end 部门,
case when a.客户=b.金蝶名称 then 0 else sum(运费) end 运费,
case when a.客户=b.金蝶名称 then sum(运费) else 0 end 样品运费
from dm_金蝶销售出库单标准成本 a
left join ods_样品店铺名 b on a.客户=b.金蝶名称
where a.发货仓库='深圳榴芒'
group by a.查询日期,a.日期,a.部门) e
on d.日期=e.日期

union all

select * from k1</sql>
