      <sql>delete from dm_科目费用拆分;
insert into dm_科目费用拆分
with k1 as(
select 业务主体,日期,年度,月份,科目全名,资产分类,明细科目,费用归属组,
case when 分销私域代理KA渠道商名 in ('华润OLE-sz','天成零食旅行记-sz','河北悦卡-sz','华泰易丰-sz') then '华南' 
     when 费用归属组 in ('兴趣电商部','天猫事业部','京多事业部','分销事业部','私域事业部','线下业务部','市场部') and (管理报表核算渠道名称=费用归属组 or (资产分类='销售费用' and 明细科目 not in 
		 ('销售费用_代理推广佣金','销售费用_广告促销费','销售费用_劳务','销售费用_平台其他费用','销售费用_平台佣金','销售费用_售后扣款','销售费用_寄样成本','销售费用_寄样运费','销售费用_快递费') and 季节性产品 in ('其他','未分')) )
then '部门公共费用' 
else 管理报表核算渠道名称 end 管理报表核算渠道名称,
case when 分销私域代理KA渠道商名 in ('华润OLE-sz','天成零食旅行记-sz','河北悦卡-sz','华泰易丰-sz' )then '华南' 
     when 分销私域代理KA渠道商名 in ('盒马总店' ) then '直营'
else 原生渠道 end 原生渠道,
case when 费用归属组 in ('兴趣电商部','天猫事业部','京多事业部') then 原生渠道 else 分销私域代理KA渠道商名 end 分销私域代理KA渠道商名,季节性产品,
case when 科目全名 regexp ('收入') or 科目全名='应交税费_应交增值税_销项税额' then sum(ifnull(贷方金额,0)) else sum(ifnull(借方金额,0)) end 金额
from ods_dm_费用科目表
where 费用归属组&lt;&gt;'分销事业部'  and 月份=7 and 费用归属组='兴趣电商部'
-- and 管理报表核算渠道名称 not in ('直营','线下业务部','华南','华北','华中')
group by 业务主体,日期,年度,月份,科目全名,资产分类,明细科目,费用归属组,
case when 分销私域代理KA渠道商名 in ('华润OLE-sz','天成零食旅行记-sz','河北悦卡-sz','华泰易丰-sz') then '华南' 
     when 管理报表核算渠道名称=费用归属组 and 费用归属组 in ('兴趣电商部','天猫事业部','京多事业部','分销事业部','私域事业部','线下业务部','市场部')
then '部门公共费用' 
else 管理报表核算渠道名称 end,
case when 分销私域代理KA渠道商名 in ('华润OLE-sz','天成零食旅行记-sz','盒马总店','河北悦卡-sz','华泰易丰-sz' )then '华南' end
,case when 分销私域代理KA渠道商名 in ('华润OLE-sz','天成零食旅行记-sz','河北悦卡-sz','华泰易丰-sz' )then '华南' 
     when 分销私域代理KA渠道商名 in ('盒马总店' ) then '直营'
else 原生渠道 end,季节性产品

union all 

select d.业务主体,d.日期,d.年度,d.月份,d.科目全名,d.资产分类,d.明细科目,d.费用归属组,
case when d.管理报表核算渠道名称='分销' then ifnull(c.管理报表核算渠道名称,'其他分销商') 
     when d.管理报表核算渠道名称=d.费用归属组  then '部门公共费用' else d.管理报表核算渠道名称 end 管理报表核算渠道名称 ,
d.原生渠道,d.分销私域代理KA渠道商名,d.季节性产品,d.金额
from (
select 业务主体,日期,年度,月份,科目全名,资产分类,明细科目,费用归属组,管理报表核算渠道名称,原生渠道,分销私域代理KA渠道商名,季节性产品,
case when 科目全名 regexp ('收入') or 科目全名='应交税费_应交增值税_销项税额' then sum(ifnull(贷方金额,0)) else sum(ifnull(借方金额,0)) end 金额
-- case when 科目全名='主营业务收入' then sum(ifnull(贷方金额,0)) over(partition by 年度,月份,分销私域代理KA渠道商名 ) else 0 end 月营业收入
from ods_dm_费用科目表
where 费用归属组='分销事业部'
-- and  科目全名='主营业务收入'
-- and 月份=6
group by 业务主体,日期,科目全名,资产分类,明细科目,费用归属组,管理报表核算渠道名称,原生渠道,分销私域代理KA渠道商名,季节性产品) d
left join (
select 业务主体,年度,月份,
case when 排名&lt;=10 then 分销私域代理KA渠道商名 else '其他分销商' end 管理报表核算渠道名称,费用归属组,分销私域代理KA渠道商名
from (
select 业务主体,年度,月份,管理报表核算渠道名称,费用归属组,分销私域代理KA渠道商名,金额,
row_number() over (partition by 业务主体,年度,月份 order by 金额 desc) 排名
from(
select 业务主体,年度,月份,管理报表核算渠道名称,费用归属组,分销私域代理KA渠道商名,sum(贷方金额) 金额
from ods_dm_费用科目表
where 费用归属组='分销事业部'
and 资产分类='主营业务收入'
-- and 月份=6
group by 业务主体,年度,月份,分销私域代理KA渠道商名
order by 年度,月份,金额 desc)a )b
) c
on d.业务主体=c.业务主体 and d.年度=c.年度 and d.月份=c.月份
and d.费用归属组=c.费用归属组 and d.分销私域代理KA渠道商名=c.分销私域代理KA渠道商名
)
select * from k1
-- where 费用归属组='线下业务部'
union all 

select 业务主体,日期,年度,月份,科目全名,资产分类,明细科目,费用归属组,
case when 管理报表核算渠道名称='直营' then replace(分销私域代理KA渠道商名,'-sz','') 
     when 管理报表核算渠道名称 in ('华南','华中','华北') then '经销小计'
    else 管理报表核算渠道名称 end 管理报表核算渠道名称,
原生渠道,分销私域代理KA渠道商名,季节性产品,金额
from k1
where 费用归属组='线下业务部'</sql>
