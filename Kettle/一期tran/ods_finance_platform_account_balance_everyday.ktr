    <connection>lmyknb</connection>
    <sql>SELECT
  f_date 日期
, sum(ifnull(consumed_amount,0)) 消耗金额
, case when shop='官方旗舰店（书鹏）' then '抖音旗舰店-sz'
       when shop='零食旗舰店（帝耀)' then '抖音零食店-sz'
       when shop='生鲜旗舰店（王成）' then '抖音生鲜店-sz'
	   when shop='天猫旗舰店' then '天猫旗舰店-sz'
       when shop='天猫超市' then '天猫超市-sz'
       when shop='拼多多旗舰店' then '拼多多旗舰店-sz'
       when shop='拼多多专卖店' then '拼多多专卖店-sz'
       when shop='拼多多水果店' then '拼多多鲜果店-sz'
 end 店铺
FROM lmykerp_pro.finance_platform_account_balance_everyday
group by f_date,shop</sql>
