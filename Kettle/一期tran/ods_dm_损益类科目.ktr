    <connection>Fbi</connection>
    <sql>select id,日期
,年度
,月份
,凭证号
,摘要
,科目编码
,科目全名
,核算维度
,借方金额
,贷方金额
,原币金额
,资产分类
,
case when 明细科目 ='主营业务成本' then 
	case when 核算维度 regexp('运费')  then  '主营业务成本-运费'
		 when 核算维度 regexp('货品')  then  '主营业务成本-货品'
	else '主营业务成本'
	end
else 明细科目
end as 明细科目
,管理报表核算渠道名称
,substring_index(分销私域代理KA渠道商名,'-',1) 分销私域代理KA渠道商名
,季节性产品
,费用归属组
,投资名称归属
,业务主体
,case 
	 when 业务主体 = '东莞' then '代工业务'
	 when 管理报表核算渠道名称 regexp('直营') then 'KA直营'
	 when 管理报表核算渠道名称 regexp('抖音|榴芒一刻零食旗舰店|榴芒一刻生鲜旗舰店') then '抖音'
	 when 管理报表核算渠道名称 regexp('京东旗舰店') then '京东'
	 when 管理报表核算渠道名称 regexp('京东自营') then '京东自营'
	 when 管理报表核算渠道名称 ='有赞' then '私域-有赞'
	 when 管理报表核算渠道名称 regexp('榴芒一刻旗舰店') then '天猫'
	 when 管理报表核算渠道名称 regexp('天猫超市') then '天猫超市'
	 when 管理报表核算渠道名称 regexp('华南|华北|华中|线下业务部') then 'KA分销'
	 when 管理报表核算渠道名称 regexp('私域代理|拼多多|快手|小红书|有赞美食|榴芒一刻美食店|小红书') then '其它（其它线上2C平台合计）'
	 when 管理报表核算渠道名称 regexp('分销') then '线上分销-其他的平台集合店'
end as 投资名称
from profit.ods_dm_费用科目表
where 科目全名 regexp('管理费用|销售费用|研发费用|财务费用|主营业务收入|主营业务成本|营业税金及附加|其他业务收入|其他业务成本|其他业务支出|营业外收入|营业外支出|资产减值损失')
</sql>
